{
  "extends": ["eslint:recommended"],
  "parser": "babel-eslint",
  "rules": {
    "semi": [1, "never"],
    "no-console": 0
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "classes": true,
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "node": true,
    "es6": true,
    "jest": true
  },
  "globals": {
    "window": true
  }
}
